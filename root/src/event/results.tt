{{ IF event.tallied }}
	{{
		USE cache = Cache;
		cache.inc(
			'template' => 'event/results_.tt',
			'ttl' => c.debug || 360,
			'keys' => {
				'type' => 'event-results',
				'id' => event.id,
			},
		);
	}}
{{ ELSE }}
<div class="Document">
	<div class="Document-title">
		{{ type.ucfirst }} Results
	</div>

	<div class="Countdown">
		{{ event.end.delta_html | none }}
	</div>
</div>
{{ END }}
